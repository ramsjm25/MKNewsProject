{
  "functions": {
    "api/proxy/[...path].js": {
      "maxDuration": 30
    },
    "api/mock-email-service.js": {
      "maxDuration": 30
    },
    "api/test-simple.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/test-simple",
      "destination": "/api/test-simple"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/proxy/$1"
    }
  ]
}